{
    "swagger": "2.0",
    "info": {
        "description": "Roamio Engine API routes.",
        "title": "Roamio Backend API",
        "contact": {},
        "version": "0.1"
    },
    "host": "localhost:8080",
    "paths": {
        "/register": {
            "post": {
                "description": "This endpoint allows you to create a new user by providing the required details like username, email, and password.",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "users"
                ],
                "summary": "Create a new user",
                "parameters": [
                    {
                        "description": "User Data",
                        "name": "user",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/models.User"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "User registered successfully",
                        "schema": {
                            "$ref": "#/definitions/models.SuccessResponse"
                        }
                    },
                    "400": {
                        "description": "Invalid input data",
                        "schema": {
                            "$ref": "#/definitions/models.ErrorResponse"
                        }
                    },
                    "409": {
                        "description": "Username or Email already exists",
                        "schema": {
                            "$ref": "#/definitions/models.ErrorResponse"
                        }
                    },
                    "500": {
                        "description": "Internal server error",
                        "schema": {
                            "$ref": "#/definitions/models.ErrorResponse"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "models.ErrorResponse": {
            "type": "object",
            "properties": {
                "error": {
                    "type": "string",
                    "example": "\u003cDescription of error\u003e"
                }
            }
        },
        "models.SuccessResponse": {
            "type": "object",
            "properties": {
                "message": {
                    "type": "string",
                    "example": "\u003cSuccessful completion of task\u003e"
                }
            }
        },
        "models.User": {
            "type": "object",
            "properties": {
                "DOB": {
                    "description": "Date of Birth in ISO 8601 format (yyyy-mm-dd)",
                    "type": "string",
                    "example": "yyyy-mm-dd"
                },
                "Email": {
                    "description": "Unique email address",
                    "type": "string"
                },
                "Fullname": {
                    "description": "Full name of the user",
                    "type": "string"
                },
                "Location": {
                    "description": "Location of the user",
                    "type": "string"
                },
                "Password": {
                    "description": "Password (hashed)",
                    "type": "string"
                },
                "Username": {
                    "description": "Unique username",
                    "type": "string"
                }
            }
        }
    }
}